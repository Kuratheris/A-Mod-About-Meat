<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 
  <ThingDef ParentName="SculptureBase">
    <defName>MeatSculptureSmall</defName>
    <label>small meat sculpture</label>
    <description>A torso-sized piece of meat sculpted into a weird form.</description>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/ArtSmallMeat</texPath>
	  	        <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <fillPercent>0.35</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <Mass>5</Mass>
      <Beauty>-15</Beauty>
      <WorkToMake>18000</WorkToMake>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>20</costStuffCount>
	    <costList>
      <MeatCube>50</MeatCube>
    </costList>
    <recipeMaker>
		<researchPrerequisite>MeatCubes</researchPrerequisite>
    </recipeMaker>
  </ThingDef>
  
    <ThingDef ParentName="SculptureBase">
    <defName>MeatSculptureGrand</defName>
    <label>grand meat sculpture</label>
    <description>A car-sized piece of meat sculpted into an artistic form.</description>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/ArtGrandMeat</texPath>
	  	  	        <shaderType>CutoutComplex</shaderType>
      <drawSize>(4,4)</drawSize>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <fillPercent>0.5</fillPercent>
    <rotatable>false</rotatable>
    <size>(2,2)</size>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <Beauty>-240</Beauty>
      <Mass>50</Mass>
			<WorkToMake>65000</WorkToMake>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>80</costStuffCount>
		    <costList>
      <MeatCube>250</MeatCube>
    </costList>
	  <recipeMaker>
		<researchPrerequisite>MeatCubes</researchPrerequisite>
    </recipeMaker>
  </ThingDef>
  
</Defs>